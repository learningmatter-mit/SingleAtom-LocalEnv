        "n_h": {
            "target": 4
        },
        "readout_dropout": {
            "target": 0.05
        },
        "fc_dropout": {
            "target": 0.05
        },
        "n_outputs": {
            "target": 1
        },
        "loss_coeff": {
            "target": 1.0
