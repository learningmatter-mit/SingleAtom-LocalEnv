{
    "wandb": {
        "project": "per-site learning sigopt (perov)",
        "name": "phonon learning"
    },
    "details": {
        "multifidelity": false,
        "output_keys": [
            "magmom",
            "bandfilling",
            "bandcenter",
            "bandwidth"
        ],
        "loss_fn": "MSE",
        "metric_fn": "MAE",
        "optim": "Adam",
        "lr": 0.0001,
        "weight_decay": 0.2,
        "sched": "reduce_on_plateau",
        "val_size": 0.2,
        "test_size": 0.2
    },
    "modelparams": {
        "feat_dim": 64,
        "activation": "learnable_swish",
        "learnable_k": true,
        "activation_f": "softplus",
        "n_rbf": 20,
        "cutoff": 6.5,
        "num_conv": 3,
        "conv_dropout": 0.05,
        "atom_fea_len": {
            "target": 128
        },
        "h_fea_len": {
            "target": 256
        },
        "n_h": {
            "target": 4
        },
        "readout_dropout": {
            "target": 0.05
        },
        "fc_dropout": {
            "target": 0.05
        },
        "n_outputs": {
            "target": 4
        },
        "loss_coeff": {
            "target": 1.0
        }
    }
}